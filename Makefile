valgrind_run:
	valgrind ./philo 5 800 200 200 5

valgrind: $(TARGET)
	valgrind ./$(TARGET)

helgrind: $(TARGET)
	valgrind --tool=helgrind ./$(TARGET) 15 1000 50 50 5

fclean: clean
	rm -f $(NAME) $(BONUS)

sanitize: $(OBJS)
	$(CC) $(FLAGS) $(SANITIZE_FLAGS) $(INCLUDE) -o $(NAME)_sanitize $(OBJS)

re: fclean all

%.o: %.c $(HEADER)
	$(CC) $(FLAGS) -c $<  -o $@
